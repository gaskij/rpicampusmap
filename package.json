{
  "name": "rpicampusmap",
  "version": "1.0.0",
  "description": "An interactive map for the Rensselaer Polytechnic Institute campus.",
  "main": "app.js",
  "private": true,
  "scripts": {
    "api-dev": "ts-node-dev --respawn --transpileOnly ./src/api/server.ts",
    "api-prod": "tsc && node dist/api/server.js",
    "lint": "eslint --ext .js,.ts \"./src/**/*.{js,ts}\"",
    "lint-fix": "npm run lint -- --fix",
    "start": "npm run ui-prod && npm run api-prod",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "ui-dev": "webpack-dev-server --history-api-fallback --inline --progress --open --watch",
    "ui-prod": "webpack --bail --progress --profile",
    "update-db": "node update_db.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gaskij/rpicampusmap.git"
  },
  "keywords": [
    "JavaScript"
  ],
  "author": "Justin Gaskins",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/gaskij/rpicampusmap/issues"
  },
  "homepage": "https://github.com/gaskij/rpicampusmap#readme",
  "dependencies": {
    "@types/angular": "^1.6.57",
    "@types/angular-route": "^1.7.0",
    "@types/bootstrap": "^4.3.1",
    "@types/jquery": "^3.3.33",
    "@types/leaflet": "^1.5.9",
    "angular": "^1.7.9",
    "angular-animate": "^1.7.9",
    "angular-mocks": "^1.7.9",
    "angular-resource": "^1.7.9",
    "angular-route": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.4.1",
    "connect-flash": "^0.1.1",
    "dotenv": "^7.0.0",
    "ejs": "^2.7.4",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.5.0",
    "express-session": "^1.17.0",
    "jquery": "^3.4.1",
    "leaflet": "^1.6.0",
    "mongodb": "^3.2.3",
    "mongoose": "^5.8.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "@types/angular-mocks": "^1.7.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.3",
    "@types/mongoose": "^5.7.1",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "bufferutil": "^4.0.1",
    "canvas": "^2.6.1",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.5.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "nodemon": "^1.19.1",
    "rimraf": "^3.0.2",
    "style-loader": "^1.1.3",
