{
  "name": "campusmapapi",
  "version": "1.0.0",
  "description": "The backend API for the RPICampusMap project.",
  "private": true,
  "scripts": {
    "api-dev": "ts-node-dev --respawn --transpileOnly ./src/server.ts",
    "api-prod": "npm run build && node dist/server.js",
    "build": "tsc",
    "client": "cd ../campusmap && npm start",
    "dev": "concurrently -n 'server,client' -c 'blue,purple' \"npm run api-dev\" \"npm run client\"",
    "lint": "eslint --ext .js,.ts \"./src/**/*.{js,ts}\"",
    "lint-fix": "npm run lint -- --fix",
    "start": "npm run api-dev",
    "update-db": "node update_db.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gaskij/rpicampusmap.git"
  },
  "author": "Justin Gaskins",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/gaskij/rpicampusmap/issues"
  },
  "homepage": "https://github.com/gaskij/rpicampusmap#readme",
  "dependencies": {
    "connect-flash": "^0.1.1",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "ejs": "^2.7.4",
    "express": "^4.17.1",
    "mongodb": "^3.2.3",
    "mongoose": "^5.8.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/mongoose": "^5.7.1",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "concurrently": "^5.3.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.5.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  }
}
